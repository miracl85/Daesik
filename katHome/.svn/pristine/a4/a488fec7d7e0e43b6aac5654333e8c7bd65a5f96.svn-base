/* text transition
****************************************************************** */
/*.visual_txt strong,
.visual_txt p,
.visual_txt h2,
.visual_txt a*/
.visual_txt > *,
.visual_txt2 > *,
.visual_txt3 > *,
.visual_txt4 > * {
	-webkit-transition: all 1s ease-out;
	-moz-transition: all 1s ease-out;
	-o-transition: all 1s ease-out;
	transition: all 1s ease-out;
}
.visual_txt h2,
.visual_txt2 h2,
.visual_txt3 h2,
.visual_txt4 h2 {
	margin-top:30px;
	opacity:0;
}
.visual_txt p,
.visual_txt2 p,
.visual_txt3 p,
.visual_txt3 p {
	margin-top:40px;
	opacity:0;
}
.visual_txt .btn_view,
.visual_txt2 .btn_view,
.visual_txt3 .btn_view,
.visual_txt4 .btn_view {
	margin-top:50px;
	opacity:0;
}

.visual_txt.active > *,
.visual_txt2.active > *,
.visual_txt3.active > *,
.visual_txt4.active > * {
	margin-top:0;
	opacity:1;
}


/* 페이징 */
