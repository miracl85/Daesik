}
nav > ul > li {
	position:relative;
	display:inline-block;
}
nav > ul > li > a {
	display:block;
	padding:0 40px;
	color:#fff;
	font-size:1em;
}
nav > ul > li > a::before {
	content:"";
	position:absolute;
	top:1.9em;
	left:0;
	display:block;
	width:3px;
	height:3px;
	vertical-align:middle;
	border-radius:20px;
