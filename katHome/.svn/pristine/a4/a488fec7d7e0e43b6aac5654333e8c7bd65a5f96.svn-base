@charset "utf-8";
@import url('reset.css');


/* Centered texts in each section
* --------------------------------------- */
.section{
	text-align:center;
}


/* Backgrounds will cover all the section
* --------------------------------------- */
#section0,
#section1,
#section2,
#section3 {
	padding-top:4em;
	background-size: cover;
}

/* Defining each sectino background and styles
* --------------------------------------- */
#section0{
    height: 959px;
    /* background: url(../img/main/main01.jpg) no-repeat center; */
    background-size: cover;
    overflow:hidden;
}
#section0 h1{
	top: 50%;
	transform: translateY(-50%);
	position: relative;
}
#section1 {
	background-image: url(../img/main/bg_main_video_capture.jpg);
	overflow:hidden;
	width:100%;
}
#section2 {
	background-image: url(../img/main/bg_main_content.png);
	background-position:center center;
	display:table;
	width:100%;
}
#section3 {
	padding:0;
	height:auto !important;
	text-align:left;
}

/* section 세로 중앙정렬 */
.section_wrap_box{
	display:table;
	width:100%;
	height:100%;
}
.section_middle_box{
	display:table-cell;
	vertical-align:middle;
}
#section2 .section_middle_box{
	padding-bottom:3em;
}



/*Adding background for the slides
* --------------------------------------- */
#slide1{
	background-image: url(../img/bg2.jpg);
	padding: 6% 0 0 0;
}
#slide2{
	background-image: url(../img/bg5.jpg);
	padding: 6% 0 0 0;
}


/* Bottom menu
* --------------------------------------- */
#infoMenu li a {
	color: #fff;
}


/* Section0 background Video 
* --------------------------------------- */
.bg_video1 {
    position:absolute;
	top: 50%;
    left: 50%;
    min-width: 100%;
    min-height: 100%;
    width: auto;
    height: auto;
  	background-size: cover;
  	transform: translateX(-50%) translateY(-50%);
  	-webkit-transform: translateX(-50%) translateY(-50%);
  	-moz-transform: translateX(-50%) translateY(-50%);
  	-o-transform: translateX(-50%) translateY(-50%);
  	transition: 1s opacity;
  	-webkit-transition: 1s opacity;
  	-moz-transition: 1s opacity;
  	-o-transition: 1s opacity;
  	z-index:0;
}


/* Overwriting fullPage.js tooltip color
* --------------------------------------- */
.fp-tooltip{
