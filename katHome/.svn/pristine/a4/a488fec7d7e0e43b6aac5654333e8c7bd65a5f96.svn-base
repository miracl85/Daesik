/* site map
================================================*/
.site_map {
	position:fixed;
	top:0;
	right:0;
	bottom:0;
	left:0;
	background-color:rgba(0, 0, 0, 0.5);
	z-index:999;
}
.site_map_body {
	position:absolute;
	top:50%;
	left:50%;
	margin-top:-185px;
	margin-left:-475px;
	padding-bottom:80px;
	width:60em;
	border:3px solid #ff6f22;
	border-width:0 3px 3px 3px;
	background-color:#fff;
	box-sizing:border-box;
}
.site_map_body::before {
	content:"";
	display:block;
	position:absolute;
	top:0;
	right:0;
	left:0;
	height:5.2em;
	background-color:#ff6f22;
}
.site_part {
	position:relative;
	float:left;
	width:33.33%;
}
.site_part::after {
	content:"";
	display:block;
	clear:both;
	*zoom:1;
}
.site_part::before {
	content:"";
	position:absolute;
	top:83px;
	right:-1px;
	display:block;
	width:1px;
	height:235px;
	background-color:#ddd;
}
.site_part.last::before {
	top:-999999em;
	width:0;
	height:0;
	background-color:transparent;
}
.site_part .site_topic {
	position:relative;
	padding:20px;
	border-left:1px solid #d84b00;
	background-color:#ff6f22;
}
.site_part:first-child .site_topic {
	border-left:#ff6f22;
}
.site_topic::before {
	content:"";
	position:absolute;
	top:12px;
	right:15px;
	display:block;
	width:60px;
	height:60px;
	background:url(../img/pup/ico_site_map.gif) no-repeat 0 0;
	background-size:100%; 
}
.site_topic.company::before {
	background-position:0 0;
	background-size:60px;
}
.site_topic.business::before {
	background-position:0 -60px;
	background-size:60px;
}
.site_topic.community::before {
	background-position:0 -120px;
	background-size:60px;
}

.site_part .site_topic strong {
	color:#fff;
	font-size:19px;
	font-weight:500;
}
.site_part .site_topic p {
	padding-bottom:5px;
	color:#ffad81;
	font-size:14px;
	letter-spacing:0;
	text-transform:uppercase;
}
.site_part ul {
	padding:20px;
}
.site_part li {
	margin-top:13px;
}
.site_part li:first-child {
	margin-top:0;
}
.site_part li a {
	font-weight:14px;
}
.site_part li a:hover {
	color:#ff6f22; 
}

.business_ul {
	float:left;
	width:50%;
	box-sizing:border-box;
}
.business_ul.business_left {
	padding:0 10px 0 20px;
}
.business_ul.business_right {
	padding:0 20px 0 10px;
}
.business_ul strong {
	display:block;
	padding:20px 5px 10px;
	color:#ff6f22;
	font-weight:600;
	border-bottom:1px solid #ff6f22;
}
.site_part .business_ul ul {
	padding:15px 0;	
}

/* 닫기 */
.btn_site_close {
	position:absolute;
	top:-60px;
	right:-3px;
	display:block;
	width:60px;
	height:60px;
	font-size:0;
	text-indent:-9999em;
	background:#ef5300 url(../img/pup/ico_site_close.gif) no-repeat 0 0;
	background-size:60px;
}


/* 사이트맵 - 유틸 */
.site_util{
	position:absolute;
	left:0;
	right:1px;
