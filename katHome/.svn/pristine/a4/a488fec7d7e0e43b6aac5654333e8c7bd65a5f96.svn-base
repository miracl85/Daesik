.main_video_gp.active{
	opacity:1;
	border:1px solid rgba(255,255,255,0.4);
	transition:all .5s linear;
	-webkit-transition: all 0.4s linear;
	-moz-transition: all 0.4s linear;
	-ms-transition: all 0.4s linear;
}
.main_video{
