	height:1px;
	background-color: #fff;
	z-index:4;
}
/* lin_gp_left-right (원형) */
.kat_lin_gp_left{
	position:absolute;
	top:0px;
	left:0px;
	right:50%;
	width:50%;
	height:100%;
	overflow:hidden;
	z-index:3;
}
.kat_lin_gp_right{
	position:absolute;
	top:0px;
	left:50%;
	right:0px;
	width:50%;
	height:100%;
	overflow:hidden;
	z-index:3;
}
.lin_inner{
	position:absolute;
	top:10px;
	left:10px;
	bottom:10px;
	width:300px;
	height:300px;
	border-radius:300px;
	box-sizing: border-box;
	border:1px solid #fff;
	display:none;
}
.lin_inner_02{
	position:absolute;
	top:10px;
	left: -150px;
	bottom:10px;
	width:300px;
	height:300px;
	border-radius:300px;
	box-sizing: border-box;
	border:1px solid #fff;
	display:none;
}
@keyframes kat_lin_left{
	0%{
		width:0;
	}
	100%{
		width:50%;
	}
}
@keyframes kat_lin_right{
	0%{
		width:0;
	}
	100%{
		width:50%;
	}
}

/* lin_gp_full */
.kat_lin_gp_full{
	display:none;
	position:absolute;
	top:10px;
	left:10px;
	right:10px;
	bottom:10px;
	overflow:hidden;
	z-index:10;
}
.kat_lin_gp_full .lin_inner_top{
	position:absolute;
	display:block;
	top:0px;
	left:0px;
	right:0px;
	width:0%;
	height:1px;
	background: #fff;
	opacity:1;
	transition: all .5s ease-out;
	overflow:hidden;
}
.kat_lin_gp_full .lin_inner_bottom{
	position:absolute;
	display:block;
	bottom:0px;
	left:0px;
	width:0%;
	height:1px;
	background: #fff;
	opacity:1;
	transition: all .5s ease-out;
	overflow:hidden;
}
.kat_lin_gp_full:hover .lin_inner_top,
.kat_lin_gp_full:hover .lin_inner_bottom{
	width:100%;
	opacity:1;
	transition: all .5s ease-out;
}
.kat_lin_gp_full .lin_inner_left{
	position:absolute;
	display:block;
	top:0px;
	left:0px;
	height:0%;
	width:1px;
	background: #fff;
	opacity:1;
	transition: all .5s ease-out;
	overflow:hidden;
}
.kat_lin_gp_full .lin_inner_right{
	position:absolute;
	display:block;
	top:0px;
	right:0px;
	height:0%;
	width:1px;
	background: #fff;
	opacity:0;
	transition: all .5s ease-out;
	overflow:hidden;
}
.kat_lin_gp_full:hover .lin_inner_left,
.kat_lin_gp_full:hover .lin_inner_right{
	height:100%;
	opacity:1;
	transition: all .5s ease-out;
}

/* btn_more - 플러스 버튼 */
.btn_more{
	position: absolute;
	left: 50%;
	margin-left:-23px;
	bottom: 0;
	display: block;
	width:46px;
	height:46px;
	background: rgba(0, 0, 0, 0.3);
	font-size:0;
	-webkit-transition: all 0.4s linear;
	-moz-transition: all 0.4s linear;
	-ms-transition: all 0.4s linear;
	transition: all 0.4s linear;
	border-radius:50px;
	z-index:12;
}
.btn_more div{
	position:relative;
	width:100%;
	height:100%;
	transition: .5s;
}
.main_video .btn_more{
	border-radius:0;
	bottom:0;
	right:0;
	left:auto;
	margin:0;
	display: none;
}
/* hover 효과 */
.kat_box:hover .box_thumb:after{
	opacity:1;
}
.kat_box:hover .kat_txt,
.kat_box:hover .lin_inner,
.kat_box:hover .lin_inner_02{
	display:block;
}
.kat_box:hover .kat_lin_gp_left{
	animation: kat_lin_left 1s;
}
.kat_box:hover .kat_lin_gp_right{
	animation: kat_lin_right 1s;
}
.kat_box:hover .kat_tit span{
	color:#ffd7c2;
	background-color:transparent;
}
/* hover 효과 - btn_more 플러스 버튼 */
.kat_box:hover .btn_more{
	background: #eb7030;
}
.kat_box:hover .btn_more div,
.main_video:hover .btn_more div{
	transform: rotate(180deg);
	-webkit-transform: rotate(180deg);
	-moz-transform: rotate(180deg);
	-ms-transform: rotate(180deg);
}
.kat_box .btn_more div img,
.main_video .btn_more div img{
	display:block;
	width:100%;
	height:100%;
	float:left;
}
.main_video.mv01:hover .btn_more{
	background: #234c8f;
}
.main_video.mv02:hover .btn_more{
	background: #0ec4da;
}
.main_video.mv03:hover .btn_more{
	background: #bc9087;
}
.main_video.mv04:hover .btn_more{
	background: #b7bbbe;
}
.thevideo_box{
	width:100%;
	height:100%;
}

