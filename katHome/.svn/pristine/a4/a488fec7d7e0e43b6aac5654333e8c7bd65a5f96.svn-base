/* main bottom scroll */
.scroll_simbol {
	position:absolute;
	bottom:20px;
	left:0;
	right:0;
	z-index:100;
}
.scroll_box {
	position:relative;
	margin:0 auto;
	width:50px;
	height:50px;
	border-radius:100%;
	background-color:rgba(255, 111, 34, 0.9);
}
/* scroll main btn */
.btn_m_scroll {
	position:relative;
	display:block;
	font-size:0;
	text-indent:-999em;
	cursor: default;
}
.btn_m_scroll::before {
	content:"";
	display:block;
	width:50px;
	height:50px;
	background:url(../img/comm/ico_m_arrow.png) no-repeat center;
	background-size:100%;
}
/* scroll ball scale keyframes */
.ball_scale_multiple {
	position:absolute;
	top:-27px;
	left:-27px;
	margin:0 auto;
}
.ball_scale_multiple span {
	position:absolute;
	top:0;
	width:100px;
	height:100px;
	border:2px solid #ff6f22;
	border-radius:100%;
	-webkit-animation: ball_scale_multiple 3s 0s linear infinite;
	-moz-animation: ball_scale_multiple 3s 0s linear infinite;
	-o-animation: ball_scale_multiple 3s 0s linear infinite;
    animation: ball_scale_multiple 3s 0s linear infinite;
	opacity:0;
}
.ball_scale_multiple span:nth-child(2) {
    -webkit-animation-delay: -2s;
    -moz-animation-delay: -2s;
    -o-animation-delay: -2s;
    animation-delay: -2s; 
}
.ball_scale_multiple span:nth-child(3) {
    -webkit-animation-delay: -0.5s;
    -moz-animation-delay: -0.5s;
    -o-animation-delay: -0.5s;
    animation-delay: -0.5s; 
}

@keyframes ball_scale_multiple {
	0% {
		-webkit-transform: scale(0.3);
		-moz-transform: scale(0.3);
		-o-transform: scale(0.3);
        transform: scale(0.3);
		opacity: 0.3; 
	}
	5% {
		opacity: 0.8; 
	}
	100% {
		-webkit-transform: scale(1);
		-moz-transform: scale(1);
		-o-transform: scale(1);
        transform: scale(1);
		opacity: 0; 
	}  
}

@-webkit-keyframes ball_scale_multiple {
	0% {
		-webkit-transform: scale(0.3);
		-moz-transform: scale(0.3);
		-o-transform: scale(0.3);
        transform: scale(0.3);
		opacity: 0.3; 
	}
	5% {
		opacity: 0.8; 
	}
	100% {
		-webkit-transform: scale(1);
		-moz-transform: scale(1);
		-o-transform: scale(1);
        transform: scale(1);
		opacity: 0; 
	}  
}

