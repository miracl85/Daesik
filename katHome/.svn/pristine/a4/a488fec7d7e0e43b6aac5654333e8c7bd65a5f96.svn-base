/* footer */
footer {
	position:relative;
	color:#dbdbdb;
	font-style: normal;
	background-color:#4a4a4a;
}
footer > * {
	font-size:14px;
	color:#dbdbdb;
	font-style: normal;
	vertical-align: middle;
}
footer address {
	padding:25px;
}
footer address:after {
	content:"";
	display:block;
	clear:both;
	*zoom:1;
}
footer address div {
	float:left;
}
footer address img {
	float:right;
	clear:right;
}
footer address img.margin_t{
	margin-top:10px;
	height:50px;
}
footer address p,
footer address .email {
	position:relative;
	padding-left:15px;
	padding-bottom:3px;
	letter-spacing:0;
}
footer address .email {
	display:block;
}
footer address p:before,
footer address .email:before {
	content:"";
	display:block;
	width:3px;
	height:3px;
	background-color:#ddd;
	position:absolute;
	left:0;
	top:7px;
	border-radius:20px;
}
footer address a {
	color:#dbdbdb;
}
footer address p.copy {
	padding-top:15px;
	padding-bottom:0;
	font-size:13px;
	color:#adadad;
}
footer address p.copy:before {
	background:none;
}

footer address p span {
	padding-left:10px;
}
footer address p span a {
	padding-right:10px;
}
footer address p span a,
footer address .email {
	color:#dcaa15;
}
footer .foot_info {
	padding:7px 25px;
	border-bottom:1px solid #575757;
	background-color:#3a3a3a;
}
footer .foot_info > *{
	vertical-align:middle;
}
footer .foot_info:after {
	content:"";
	display:block;
	clear:both;
	*zoom:1;
}
footer .foot_info li {
	float:left;
	position:relative;
	margin-left:30px;
	padding-left:30px;
}
footer .foot_info li > *{
	vertical-align:middle;
}
footer .foot_info li:first-child {
	margin-left:0;
	padding-left:0;
}
footer .foot_info li:before {
	content:"";
	display:block;
	position:absolute;
	top:10px;
	bottom:2px;
	left:0;
	border-left:1px solid #616161;
}
footer .foot_info li:first-child:before {
	border-left:0;	
}
footer .foot_info li.fl_right{
	float:right;
	position:relative;
}
footer .foot_info li.fl_right:before{
	display:none;
}
footer .foot_info li a {
	display:block;
	height:24px;
	line-height:27px;
	font-size:15px;
	color:#d1d1d1;
	letter-spacing:0;
	padding-right:15px;
