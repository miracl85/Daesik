/* 오시는길 - 탭 */
.location_tab{
	position:relative;
	display:inline-block;
	width:25%;
	font-size:18px;
	padding:14px 0px;
}
.location_tab:after{
	position: absolute;
	right: -3%;
	top:25px;
	content: "";
	display: block;
	width:5px;
	height: 5px;
	background-color:#ddd;

}
.location_tab:nth-of-type(3){
	margin-right:0;
}
.location_tab:nth-of-type(3):after{
	display: none;
}
.location_tab_cont{
	border-top: 2px solid #ff7a22;
	margin-bottom:80px;
	display: none;
}
.location_cont input{
	display: none;
}
.location_cont input:nth-of-type(1):checked ~ label:nth-of-type(1),
.location_cont input:nth-of-type(2):checked ~ label:nth-of-type(2),
.location_cont input:nth-of-type(3):checked ~ label:nth-of-type(3){
	font-weight: 600;
	color:#ff7a22;
	background:url(../img/company/ico_location_arrow.png) no-repeat center bottom;
    background-size: 30px;
}
.location_cont input:nth-of-type(1):checked ~ div:nth-of-type(1),
.location_cont input:nth-of-type(2):checked ~ div:nth-of-type(2),
.location_cont input:nth-of-type(3):checked ~ div:nth-of-type(3){
	display: block;
}
