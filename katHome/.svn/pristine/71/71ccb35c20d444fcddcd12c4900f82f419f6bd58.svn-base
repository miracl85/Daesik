.ico_change span{
	display: block;
	padding-top:110px;
	font-size:22px;
	font-weight: 600;
	background: url(../img/company/ico_change.png) no-repeat center 30px;
	background-size: 50%;
}
.box_change{
	position: absolute;
	top:150px;
	width:260px;
}
.box_change:after {
    position: absolute;
