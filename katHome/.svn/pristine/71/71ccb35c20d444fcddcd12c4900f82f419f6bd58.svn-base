}
.ico_communication span{
	display: block;
	padding-top:110px;
	font-size:22px;
	font-weight: 600;
	background: url(../img/company/ico_communication.png) no-repeat center 30px;
	background-size: 50%;
}
.box_communication{
	position: absolute;
	bottom:0px;
	left:50%;
	margin-left:-130px;
	width:260px;
}
.box_communication:after {
    position: absolute;
    top: -38px;
    right: 110px;
    content: "";
    display: block;
    width: 60px;
    height: 42px;
    background: url(../img/company/earth_lin02.png) no-repeat -10px -34px;
}
.box_communication strong{
	display: block;
	width:230px;
	background-color:#54c2a4;
	color:#fff;
	text-align: center;
	font-size:22px;
	font-weight: 600;
	border-radius: 40px;
	padding:7px 0 6px;
	text-shadow:0px 0px 10px rgba(0, 0, 0, 0.3);
	margin: 0 auto;
}
.box_communication p {
	font-size:18px;
	margin-top:10px;
	word-break:keep-all;
}
