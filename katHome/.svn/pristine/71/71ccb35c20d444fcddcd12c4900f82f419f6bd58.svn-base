/* 방산사업 스펙
======================================*/
.spec_grid {
	position:relative;
	float:left;
	padding:5% 1% 5% 4%;
	width:33.3%;
	text-align:left;
	height:32.3vw;
    max-height:499px;
    color:#fff;
    box-sizing:border-box;
    cursor:pointer;
}
/* background */
.antenna {
	background:url(../img/sub/img_spac1.png) no-repeat center;
	background-size:cover;
}
.combine {
	background:url(../img/sub/img_spac2.png) no-repeat center;
	background-size:cover;
}
.system {
	background:url(../img/sub/img_spac3.png) no-repeat center;
	background-size:cover;
}
.antenna:hover::before,
.antenna.active::before {
	content:"";
	display:block;
	position:absolute;
	top:0;
	right:0;
	bottom:0;
	left:0;
	z-index:0;
	background-color:rgba(255, 122, 34, 0.4);
}
.combine:hover::before,
.combine.active::before {
	content:"";
	display:block;
	position:absolute;
	top:0;
	right:0;
	bottom:0;
	left:0;
	z-index:0;
	background-color:rgba(142, 174, 16, 0.4);
}
.system:hover::before,
.system.active::before {
	content:"";
	display:block;
	position:absolute;
	top:0;
	right:0;
	bottom:0;
	left:0;
	z-index:0;
	background-color:rgba(93, 154, 245, 0.4);
}

/* text */
.spec_grid > strong {
	position:relative;
	display:block;
	margin-bottom:40px;
	font-size:31px;
	z-index:1;
    word-break: keep-all;
}
.spec_grid > p {
	position:relative;
	display:inline-block;
	font-size:0.9em;
}
.spec_grid > p::before {
	content:"";
	position:absolute;
	bottom:-10px;
	display:block;
	width:100%;
	height:2px;
	background-color:rgba(255, 255, 255, 0.5);
}
.spec_grid > p::after {
	content:"";
	position:absolute;
	bottom:-10px;
	display:block;
	width:0%;
	height:2px;
	background-color:#fff;
	transition:all .4s ease-out;
}
.spec_grid:hover > p::after,
.spec_grid.active > p::after {
	width:100%;
}
.spec_grid > p span {
	font-size:1.02em;
	font-weight:600;
}

/* link animation
========================================*/
.lin_animation {
	display:inline-block;
	position:absolute;
	top:0;
	left:0;
	margin:10px;
	width:calc(100% - 20px);
	height:calc(100% - 20px);
	overflow:hidden;
	z-index:3;
}
.lin_animation div {
	content:'';
	display:inline-block;
	position:absolute;
	background-color:#fff;
	transition:all .4s ease-out;
	opacity:0;
	z-index:1;
}
.lin_ani_top {
	top:0;
	left:0;
	width:0%;
	height:1px;
}
.lin_ani_bottom {
	bottom:0;
	left:0;
	width:0%;
	height:1px;
}
.lin_ani_right {
	top:0;
	right:0;
	width:1px;
	height:0%;
}
.lin_ani_left {
	left:0;
	width:1px;
	height:0%;
}
.lin_animation:hover .lin_ani_top,
.lin_animation:hover .lin_ani_bottom,
.spec_grid.active .lin_animation .lin_ani_top,
.spec_grid.active .lin_animation .lin_ani_bottom {
	width:100%;
	opacity:3;
	transition:all .4s ease-in-out;
}
.lin_animation:hover .lin_ani_right,
.lin_animation:hover .lin_ani_left,
.spec_grid.active .lin_animation .lin_ani_right,
.spec_grid.active .lin_animation .lin_ani_left {
	height:100%;
	opacity:3;
	transition:all .4s ease-in-out;
}


