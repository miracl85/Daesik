@charset "utf-8" 
@import url('reset.css');

.content{
	padding-top:64px;	
}
.sub_cont_inner{
	width:80%;
	max-width:1280px;
	margin:0  auto;	
	padding:20px 0;
	text-align: center;
	min-height:800px;
}
/* 서브 상단 */
.sub_head{
	width:100%;
	height:250px;
	background: url(../img/comm/bg_main3.png) no-repeat center;
	background-size:cover;
	text-align: center;
	color:#fff;
	text-shadow:0px 0px 7px rgba(0, 0, 0, 0.9);
}
.sub_head.diffusion{
	background: url(../img/comm/bg_main2.png) no-repeat center;
	background-size: cover;
}
.sub_head.civilian{
	background: url(../img/comm/bg_main4.png) no-repeat center;
	background-size: cover;
}
.sub_head.community{
	background: url(../img/comm/bg_main5.png) no-repeat 0 -450px;
	background-size: cover;
}
.sub_head .sub_head_inner{
	position:relative;
	width:80%;
	max-width:1280px;
	padding-top:65px;
	margin:0 auto;
}
.sub_head .sub_head_inner strong{
	font-size:44px;
}
.sub_head .sub_head_inner p{
	padding-top:10px;
	line-height:23px;
	font-size:20px;
}
.sub_head .sub_head_inner a{
	position: absolute;
	top:50%;
	margin-top:10px;
	display: block;
	height:50px;
	line-height:50px;
	color:#fff;
	font-size:18px;
	text-shadow: 1px 1px 1px rgba(0,0,0,0.4);
}
.sub_head .sub_head_inner a.left_list_arrow{
	left:0;
	padding-left:85px;
}
.sub_head .sub_head_inner a.right_list_arrow{
	right:0;
	padding-right:85px;
}
.sub_head .sub_head_inner a:hover:before{
	background-color:rgba(255,89,0, 1);
}
.sub_head .sub_head_inner a:before{
	position:absolute;
	content:"";
	display:block;
	width:50px;
	height:50px;
	background: rgba(255,111,34, 0.7) url(../img/sub/ico_sub_head_arrow.png) no-repeat 3px 4px;
	border-radius:30px;
	transition:all 0.3s linear;
}
.sub_head .sub_head_inner a.left_list_arrow:before{
	left:0px;
}
.sub_head .sub_head_inner a.right_list_arrow:before{
	background-position: 5px -82px;
	right:0px;
}
.sub_head .sub_head_inner a:after{
	position: absolute;
	top:25px;
	content: "";
	display: block;
	width:30px;
	height:1px;
	background-color:#fff;
}
.sub_head .sub_head_inner a.left_list_arrow:after{
	left:50px;
}
.sub_head .sub_head_inner a.right_list_arrow:after{
	right:50px;
}

/* 회사소개 탭 */
.sub_content{
	position: relative;
}
ul.tabs{
	max-width:1280px;
	margin:0 auto;
	overflow:hidden;
}
ul.tabs li{
	display:inline-block;
	width:16.6%;
	height:44px;
	line-height: 45px;
	display: block;
	float:left;
	box-sizing: border-box;
	text-align: center;
	border:1px solid #ddd;
	border-width:0px 1px 1px 0px;
	cursor:pointer;
	background-color:#fdfdfd;
}
ul.tabs li:nth-child(1){
	border-left:1px solid #ddd;
	width:17%;
}
ul.tabs li.active{
	background-color:#ff6f22;
	border-color:#ff6f22; 
	color: #fff;
/* 	font-weight:600; */
}
.tab-content{
	display: none;
	text-align:center;
}
.tab-content.active{
	display: inherit;
}


/* 작업중 안내 메세지 */
.nodata_box{
	text-align: center;
}
.nodata_img{
	display: block;
	width:100%;
	height:330px;
	background: #fdfdfd url(../img/sub/img_nodata.png) no-repeat bottom center;
	border-bottom:2px solid #896549;
}
.nodata_box strong{
	display: block;
	font-size:36px;
	color:#ff7a22;
	font-weight: 600;
	margin-top:30px;
}
.nodata_box p{
	font-size:17px;
	margin-top:20px;
}
.nodata_box .btn_main_arrow{
	display: inline-block;
	padding:8px 50px 10px 30px;
	background: url(../img/sub/ico_main_arrow.png) no-repeat 150px center;
	border:1px solid #ff7a22;
	border-radius:3px;
	margin-top:40px;
	margin-bottom:20px;
}

/* 리모콘 */
.remote{
	position:absolute;
	top:60px;
	right:2%;
	border-top: 3px solid #f57c3e;
	z-index:10;
}
.remote a {
    display: block;
    width: 65px;
    height: 85px;
    line-height: 16px;
    padding: 43px 10px 0;
    box-sizing: border-box;
    background: #30353f url(../img/sub/ico_remote.png) no-repeat center 0;
    color: #fff;
    font-size: 12px;
    border-bottom: 1px dotted #797c81;
    text-align: center;
    font-size:11px;
}
.remote a.btn_company_download {
    background-position: center 4px;
}
.remote a.btn_company_video {
    background-position: center -87px;
}
.remote a.btn_company_groupware {
    background-position: center -187px;
    padding-top: 50px;
}
.remote a.btn_company_share {
    background-position: center -315px;
    padding-top: 41px;
    height: 65px;
    border-bottom: 0;
    float:right;
}
.share_util {
	position:relative;
    height:65px;
}
.share_util:hover .share_form{
	display: block;
}
.share_form {
	position:absolute;
	right:65px;
    width:130px;
    height: 66px;
    background-color: #f6f6f6;
    z-index: 2;
    float:left;
    margin-top:-1px;
    display:none;
}
.share_form li {
    float: right;
}
.share_form li a {
	display:block;
	height:66px;
	font-size:13px;
	border:1px solid #9ea5b2;
	border-width:1px 0px 1px 1px;
	background:url(../img/sub/ico_sns.png) no-repeat 0 0; 
	padding:43px 5px 0;
	color:#525965;
}
.share_form li a.btn_facebook {
	background-position:-1px -7px;
}
.share_form li a.btn_facebook:hover {
	border:1px solid #3b5998;
	background-position:-1px -91px;
	color:#fff;
}
.share_form li a.btn_twitter {
	background-position:-64px -8px;
}
.share_form li a.btn_twitter:hover {
	border:1px solid #3fa9f5;
	background-position:-65px -92px;
	color:#fff;
}
.remote a.btn_top {
    border-radius: 40px;
    width: 55px;
    height: 55px;
    padding-top: 25px;
    background: #f57c3e url(../img/sub/ico_up_arrow.png) no-repeat center 13px;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, .3);
    margin:10px 5px 0;
    font-size: 14px;
}

/* 커뮤니티 - 공지사항 */

/* 공지사항 - 상단 검색 */
.community_search{
	padding: 30px 40px;
	background-color:#f8f6f9;
	text-align: center;
}
.community_search p{
	text-align: left;
	font-size:15px;
}
.search_value{
	width:100%;
	margin-top:10px;
	overflow:hidden;
}
.search_value .period{
	width:30%;
	float:left;
}
.search_value .period a{
	display: inline-block;
	width:32%;
	height:36px;
	line-height: 34px;
	border:1px solid #c3c1c4;
	background-color:#fefefe;
	font-size: 14px;
	margin-right:2%;
	float:left;
	box-sizing: border-box;
}
.search_value .period a:last-child{
	margin-right:0;
}
.search_value .period a.selected{
	color:#ff7a22;
	border:1px solid #ff7a22;
}
.search_value .date{
	width:34%;
	height:36px;
	float:left;
	margin: 0 1%;
}
.search_value .date div{
	display: inline-block;
	width:44%;
	box-sizing: border-box;
	border:1px solid #c3c1c4;
	background-color:#fff;
}
.search_value .date div > *{
	vertical-align: middle;
}
.search_value .date div input{
	display: inline-block;
	width:100%;
	height:34px;
	font-size:14px;
	background:url(../img/sub/ico_date.gif) no-repeat 93% 7px;
	border:0;
	padding:0 30px 0 10px;
	box-sizing: border-box;
	cursor:pointer;
}
.search_value .item_search{
	width:34%;
	overflow:hidden;
	float:left;
}
.search_value .item_search .item{
	position: relative;
	width:70%;
	display:inline-block;
	float:left;
}
.search_value .item_search .item input{
	display: block;
	width:100%;
	height:36px;
	line-height:34px;
	border:0;
	border:1px solid #c3c1c4;
	font-size:14px;
	padding-left:10px;
}
.search_value .item_search .item .search_label{
	position:absolute;
	left:10px;
	top:10px;
	font-size:14px;
}
.search_value .item_search .top_search_btn{
	width:28%;
	height:36px;
	line-height:34px;
	background-color:#434343;
	color:#fff;
	border:0;
	float:right;
	cursor:pointer;
}

/* 공지사항 - 하단 검색 */
.bottom_search{
    padding: 20px;
    background-color: #f8f6f9;
    text-align: center;
    border:1px solid #c1c3c4;
    overflow:hidden;
    display: none;
}
.bottom_search .item_search{
	width:100%;
	overflow:hidden;
	float:left;
}
.bottom_search .item_search .item{
	position: relative;
	width:70%;
	display:inline-block;
	float:left;
}
.bottom_search .item_search .item input{
	display: block;
	width:100%;
	height:36px;
	line-height:34px;
	border:0;
	border:1px solid #c3c1c4;
	font-size:14px;
	padding-left:10px;
}
.bottom_search .item_search .item .search_label{
	position:absolute;
	left:10px;
	top:7px;
	font-size:14px;
}
.bottom_search .item_search .top_search_btn{
	width:28%;
	height:36px;
	line-height:34px;
	background-color:#434343;
	color:#fff;
	border:0;
	float:right;
}
.bottom_search .search_value{
	display: none;
}
.bottom_search .search_value p{
	font-size:12px;
	margin:10px 0;
}
.bottom_search .search_value .period{
	width:100%;
}
.bottom_search .search_value .date{
	width:100%;
	margin-top:10px;
	margin-left:0;
}
.bottom_search .search_value .date div:nth-child(1){
	float:left;
}
.bottom_search .search_value .date span{
	margin: 5px 2% 0;
	float:left;
}

.btn_open{
    position: relative;
    display: none;
    width: 100px;
    height: 30px;
    line-height: 30px;
    background: #4b4c56 url(../img/sub/ico_arrow_down.png) no-repeat 70px 5px;
    color: #fff;
    font-size: 14px;
    padding-right: 20px;
    border: 0;
    cursor: pointer;
    margin-bottom: 20px;
    margin-top: -1px;
}
.btn_open.active{
	background: #4b4c56 url(../img/sub/ico_arrow_up.png) no-repeat 70px 5px;
}
/* 공지사항 박스 */
.notice_wrap{
	width:100%;
	overflow:hidden;
	padding-top:20px;
}
.notice_box{
	width:23.5%;
	margin-right:2%;
	margin-bottom:2%;
	border:1px solid #c3c1c4;
	padding:20px;
	float:left;
	box-sizing: border-box;
	cursor: pointer;
}
.notice_box:nth-child(4n){
	margin-right:0;
}
.notice_box.top_notice{
	border:1px solid #ff7a22;
}
.notice_box .bullet_notice{
	display: block;
	width:90px;
	height: 30px;
	line-height: 28px;
	background-color:#ff7a22;
	color:#fff;
	font-size:14px;
	text-align: center;
	box-sizing: border-box;
	border:1px solid #ff7a22;
}
.notice_box .bullet_notice.news{
	background-color:#0090c6;
	border:1px solid #0090c6;
}
.notice_box strong{
	display: block;
	text-align: left;
	font-size: 18px;
	padding:30px 0px;
	text-overflow: ellipsis;
	white-space: nowrap;
	word-wrap:normal;
	overflow: hidden;
	font-weight: 600;
}
.notice_box .txt_box{
	display: block;
	overflow:hidden;
}
.notice_box .txt_box p{
	height:86px;
	font-size:15px;
	line-height:1.5em;
	word-break:keep-all;
	text-align:left;
}
.notice_box .btn_view_more {
    display: block;
    width: 20px;
    height: 20px;
    text-indent: -9999999px;
    background: url(../img/sub/ico_plus.png) no-repeat center;
    cursor: pointer;
    margin: 10px 0px;
}
.notice_box:hover{
	background-color:#ff7a22;
	color:#fff;
}
.notice_box:hover .bullet_notice{
	border:1px solid #fff;
	background-color:transparent;
}
.notice_box:hover .txt_box p{
	color:#fff;
}
.notice_box:hover .btn_view_more{
	background: url(../img/sub/ico_plus_hover.png) no-repeat center;
}
/* 공지사항 - 노데이터 */ 
.nodata_notice_box{
	width:100%;
	height:273px;
	background:#f6f4f7 url(../img/company/img_nodata.png) no-repeat center 20px;
	float:left;
    border: 1px solid #c3c1c4;
    padding-top: 180px;
    box-sizing: border-box;
    color:#6d6d6d;
    margin-bottom:2%;
}
.nodata_notice_box strong{
	font-size:24px;
	font-weight:600;
}
.nodata_notice_box p{
	font-size:14px;
}

/* 회사소개 */
.company_cont{
	background: #f6f4f7 url(../img/company/img_company.png) no-repeat center 20px;
	background-size:70%;
	padding-top:70px;
	padding-bottom:50px;
}
.illust_wrap{
	position:relative;
	width:1000px;
	height:700px;
	margin:0 auto;
	box-sizing: border-box;
}
.circle_line{
	position:absolute;
	left:50%;
	top:75px;
	margin-left:-250px;
	width:500px;
	height:500px;
	background: url(../img/company/bg_virtual_color.png) no-repeat center;
	background-size:100%;
	box-sizing: border-box;
	border-radius:1000px;
	padding:8px;
	font-size:0;
}
.circle_line .earth_bg{
	width:100%;
	height:100%;
	max-width:484px;
	max-height:484px;
	background: #f6f4f7 url(../img/company/bg_earth.png) no-repeat center;
	background-size:100%;
	box-sizing: border-box;
	border-radius:1000px;
	font-size:0;
}
/* 도전 */
.ico_challenge{
	position:absolute;
	left:50%;
	top:0;
	margin-left:-87.5px;
	z-index:2;
	display: block;
	width:175px;
	height:175px;
	background: #f6f4f7 url(../img/company/bg_company_ico.png) no-repeat center;
	border:8px solid #ff9422;
	border-radius:150px;
	font-size:0;
}
.ico_challenge span{
	display: block;
	padding-top:110px;
	font-size:22px;
	font-weight: 600;
	background: url(../img/company/ico_challenge.png) no-repeat center 30px;
	background-size: 50%;
}
.box_challenge{
	position: absolute;
	top:150px;
	right:0;
	width:260px;
}
.box_challenge:after {
    position: absolute;
    top: 15px;
    left: -25px;
    content: "";
    display: block;
    width: 46px;
    height: 50px;
    background: url(../img/company/earth_lin01.png) no-repeat -44px -26px;
}
.box_challenge strong{
	display: block;
	width:230px;
	background-color:#ff9422;
	color:#fff;
	text-align: center;
	font-size:22px;
	font-weight: 600;
	border-radius: 40px;
	padding:7px 0 6px;
	text-shadow:0px 0px 10px rgba(0, 0, 0, 0.3);
	margin: 0 auto;
}
.box_challenge p {
	font-size:18px;
	margin-top:10px;
	word-break:keep-all;
}

/* 소통 */
.ico_communication{
	position:absolute;
	right:210px;
	top:380px;
	margin-left:-87.5px;
	z-index:2;
	display: block;
	width:175px;
	height:175px;
	background: #f6f4f7 url(../img/company/bg_company_ico.png) no-repeat center;
